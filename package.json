{
  "name": "atomic-crm",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "test:unit": "vitest run src/**/*.test.{ts,tsx}",
    "test:smoke": "bash tests/simple-smoke-test.sh",
    "typecheck": "./node_modules/.bin/tsc --noEmit",
    "gen:types": "node ./scripts/mcp-generate-types.cjs",
    "gen:types:force": "node ./scripts/mcp-generate-types.cjs --force",
    "dev": "vite --force",
    "dev:local": "cp .env.local .env && npx supabase start && npx supabase db reset && vite --force",
    "dev:local:skip-reset": "cp .env.local .env && npx supabase start && vite --force",
    "dev:local:stop": "npx supabase stop",
    "dev:cloud": "cp .env.cloud .env && vite --force",
    "dev:check": "npx supabase status --linked",
    "build": "./node_modules/.bin/tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint:apply": "eslint **/*.{mjs,ts,tsx} --fix",
    "lint:check": "eslint **/*.{mjs,ts,tsx}",
    "prettier:apply": "prettier --config ./.prettierrc.mjs --write --list-different \"**/*.{js,json,ts,tsx,css,md,html}\"",
    "prettier:check": "prettier --config ./.prettierrc.mjs --check \"**/*.{js,json,ts,tsx,css,md,html}\"",
    "validate:colors": "node ./scripts/validate-colors.js",
    "validate:semantic-colors": "node ./scripts/validate-semantic-colors.js",
    "ghpages:deploy": "node ./scripts/ghpages-deploy.mjs",
    "supabase:remote:init": "node ./scripts/supabase-remote-init.mjs",
    "db:migrate:new": "npx supabase migration new",
    "cache:clear": "node ./scripts/cache-invalidation.js",
    "migrate:csv": "node ./scripts/migrate-opportunities-csv.js",
    "cache:clear:dry-run": "node ./scripts/cache-invalidation.js --dry-run",
    "search:reindex": "node ./scripts/search-reindex.js",
    "validate:pre-migration": "node ./scripts/validation/run-pre-validation.js",
    "validate:pre-migration:dry-run": "node ./scripts/validation/run-pre-validation.js --dry-run",
    "search:reindex:dry-run": "node ./scripts/search-reindex.js --dry-run",
    "test:performance": "echo 'Performance tests not yet implemented. See tests/e2e/organization-hierarchies-performance.spec.ts for E2E perf tests.'",
    "test:load": "node ./scripts/load-test.js",
    "test:ci": "vitest run --reporter=verbose",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:seed:dashboard-v3": "./scripts/seed-e2e-dashboard-v3.sh",
    "test:e2e:seed:dashboard-v3:cloud": "./scripts/seed-e2e-dashboard-v3.sh --cloud",
    "mcp:test": "npx @playwright/mcp@latest --browser chromium --base-url http://127.0.0.1:5173",
    "mcp:test:ipad": "npx @playwright/mcp@latest --browser chromium --device 'iPad Pro' --viewport-size 768x1024 --base-url http://127.0.0.1:5173",
    "mcp:test:desktop": "npx @playwright/mcp@latest --browser chromium --viewport-size 1440x900 --base-url http://127.0.0.1:5173",
    "mcp:debug": "npx @playwright/mcp@latest --browser chromium --base-url http://127.0.0.1:5173 --save-trace --save-video --caps vision,pdf",
    "mcp:view-trace": "npx playwright show-trace tests/e2e/.playwright-mcp/traces/*.zip",
    "lint": "npm run lint:check && npm run prettier:check",
    "db:link": "npx supabase link --project-ref aaqnanddcqvfiwhshndl",
    "db:local:start": "./scripts/docker/supabase-docker.sh start",
    "db:local:start:raw": "npx supabase start",
    "db:local:stop": "./scripts/docker/supabase-docker.sh stop",
    "db:local:restart": "./scripts/docker/supabase-docker.sh restart",
    "db:local:reset": "npx supabase db reset",
    "db:local:seed:e2e": "psql postgresql://postgres:postgres@127.0.0.1:54322/postgres -f supabase/seed-e2e.sql",
    "db:local:status": "./scripts/docker/supabase-docker.sh status",
    "db:local:stats": "./scripts/docker/supabase-docker.sh stats",
    "db:local:cleanup": "./scripts/docker/supabase-docker.sh cleanup",
    "db:local:reclaim": "./scripts/docker/supabase-docker.sh reclaim",
    "db:local:check": "./scripts/docker/supabase-docker.sh check",
    "db:local:monitor": "./scripts/docker/monitor-resources.sh",
    "generate:seed": "npx tsx scripts/generate-seed.ts",
    "supabase:deploy": "npx supabase db push && npx supabase functions deploy",
    "db:cloud:push": "npx supabase db push",
    "db:cloud:push:dry-run": "npx supabase db push --dry-run",
    "db:cloud:diff": "npx supabase db diff --linked",
    "db:cloud:status": "npx supabase migration list --linked",
    "prod:start": "npm run build && npm run supabase:deploy && npx serve -l tcp://127.0.0.1:3000 dist",
    "prod:deploy": "npm run build && npm run supabase:deploy && npm run ghpages:deploy",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "chromatic --exit-zero-on-changes",
    "chromatic:ci": "chromatic --only-changed --exit-once-uploaded",
    "prepare": "node -e \"if (process.env.CI !== 'true') { try { require('child_process').execSync('husky', {stdio: 'inherit'}) } catch (e) { } }\"",
    "// === Development Sync Scripts ===": "",
    "dev:sync:push": "./scripts/dev/sync-local-to-cloud.sh",
    "dev:users:create": "./scripts/dev/create-test-users.sh",
    "dev:reset": "./scripts/dev/reset-environment.sh",
    "dev:verify": "./scripts/dev/verify-environment.sh",
    "// === Migration Scripts ===": "",
    "migrate:deploy": "./scripts/migration/deploy-safe.sh"
  },
