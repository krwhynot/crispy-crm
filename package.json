    "@types/papaparse": "^5.3.16",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "diacritic": "^0.0.2",
    "dompurify": "^3.2.7",
    "inflection": "^3.0.2",
    "jsonexport": "^3.2.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "lucide-react": "^0.542.0",
    "next-themes": "^0.4.6",
    "papaparse": "^5.5.3",
    "query-string": "^9.2.2",
    "ra-core": "^5.10.0",
    "ra-i18n-polyglot": "^5.10.0",
    "ra-language-english": "^5.10.0",
    "ra-supabase-core": "^3.5.1",
    "ra-supabase-language-english": "^3.5.0",
    "react": "^19.1.0",
    "react-cropper": "^2.3.3",
